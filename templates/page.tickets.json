    "c7889035-81a4-40d3-9fa3-6dc7b28f6c04": {
      "type": "image-with-text",
      "blocks": {
        "template--15086721335357__c7889035-81a4-40d3-9fa3-6dc7b28f6c04-1671478843cc01f4b4-0": {
          "type": "heading",
